    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-icons": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
